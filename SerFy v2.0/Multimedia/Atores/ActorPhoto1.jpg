b) matap_um.csh
#sed -n -e 	-> buscar uma linha do output
#awk		-> buscar campo [12] do output

#!/bin/csh -f
ps aux | nl
printf "Qual a linha do processo que pretende terminal? "
set input = `head -1`
echo You entered: $input

set processo = `ps aux | sed -n -e "$input p" | awk '{print $11}'`
set pertence = `ps aux | sed -n -e "$input p" | awk '{print $1}'`
set variable = `ps aux | sed -n -e "$input p" | awk '{print $2}'`

echo O processo $processo pertencente ao $pertence, com PID $variable vai ser terminado de forma permanente.

#echo $variable
kill $variable


c) matap_todos_do_user.csh

